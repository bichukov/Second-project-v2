class __className__ extends BitrixMigration
{

    public function up()
    {
        global $DB;
        $DB->RunSQLBatch(__DIR__.'/sql/#FNAME#-up.sql');
    }

    public function down()
    {
        global $DB;
        $DB->RunSQLBatch(__DIR__.'/sql/#FNAME#-down.sql');
    }
}